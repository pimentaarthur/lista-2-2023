using System;

    class program{

       public static void Main(String [] args){

            int d;

            d=int.Parse(Console.ReadLine());

            d = Fib(d);

            Console.WriteLine(d);
        }

       static int Fib(int n)

        {

            if (n <=2){

                return 1;

        }
        
            return Fib(n - 1) + Fib(n - 2);
        
    }

    }
        
        

        
    
